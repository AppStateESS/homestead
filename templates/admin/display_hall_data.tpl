{START_FORM}
<div class="hms">
  <div class="box">
    <div class="box-title"> <h1>{TITLE}</h1> </div>
    <div class="box-content">
        <font color="red"><i>{ERROR}</i></font>
        {HALLS}<br /><br />
        <table>
            <tr>
                <th>Name of the Hall: </th><td align="left">{HALL_NAME}</td>
            </tr>
            <tr>
                <th>Select the number of floors: </th><td align="left">{NUMBER_FLOORS}</td>
            </tr>
            <tr>
                <th>How many rooms per floor? </th><td align="left">{ROOMS_PER_FLOOR}</td>
            </tr>
            <tr>
                <th>How many bedrooms per room? </th><td align="left">{BEDROOMS_PER_ROOM}</td>
            </tr>
            <tr>
                <th>How many beds per bedroom? </th><td align="left">{BEDS_PER_BEDROOM}</td>
            </tr>
            <tr>
                <th>Select a pricing tier: </th><td align="left">{PRICING_TIER}</td>
            </tr>
            <tr>
                <th>Select a gender type: </th><td align="left">{GENDER_TYPE_1} {GENDER_TYPE_1_LABEL}</td>
            </tr>
            <tr>
                <th></th><td align="left">{GENDER_TYPE_2} {GENDER_TYPE_2_LABEL}</td>
            </tr>
            <tr>
                <th></th><td align="left">{GENDER_TYPE_3} {GENDER_TYPE_3_LABEL}</td>
            </tr>
            <tr>
                <th>Is online: </th><td align="left">{IS_ONLINE_1} {IS_ONLINE_1_LABEL}</td>
            </tr>
            <tr>
                <th></th><td align="left">{IS_ONLINE_2} {IS_ONLINE_2_LABEL}</td>
            </tr>
            <tr>
                <th>Air Conditioned: </th><td align="left">{AIR_CONDITIONED_1} {AIR_CONDITIONED_1_LABEL}</td>
            </tr>
            <tr>
                <th></th><td align="left">{AIR_CONDITIONED_2} {AIR_CONDITIONED_2_LABEL}</td>
            </tr>
        </table>
        {SUBMIT}
    </div>
  </div>
</div>
{END_FORM}
