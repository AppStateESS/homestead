<h1>Create Roommate Group - {TERM}</h1>
{START_FORM}
<div class="row">
    <div class="col-md-4">

        <div class="form-group">
            <label for="{ROOMMATE1_ID}">First roommate</label>
            {ROOMMATE1}
        </div>

        <div class="form-group">
            <label for="{ROOMMATE2_ID}">Second roommate</label>
            {ROOMMATE2}
        </div>

        <div class="form-group">
            <button type="submit" class="btn btn-success">Create Group</button>
        </div>
    </div>
</div>

{END_FORM}
